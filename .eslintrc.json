{
  "extends": "next/core-web-vitals",
  "rules": {
    "@next/next/no-img-element": "off",
    "indent": ["error", 2],
    "no-trailing-spaces": "error",
    "quotes": ["error", "single"],
    "semi": "error",
    "arrow-spacing": "error",
    "object-curly-spacing": ["error", "always"],
    "no-var": "error",
    "prefer-const": "error",
    "no-console": "error",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}
